<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/assignTicketsLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="www.siteurl.in.srsadmin.activities.AssignTicketToStaff">

    <include
        android:id="@+id/assign_tickets_toolbar"
        layout="@layout/main_toolbar" />

    <ListView
        android:id="@+id/staffList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/assign_tickets_toolbar"
        android:layout_marginTop="10dp">

    </ListView>

</RelativeLayout>

